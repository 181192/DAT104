<?xml version="1.0" encoding="UTF-8"?>
<persistence xmlns="http://java.sun.com/xml/ns/persistence" version="2.0">

    <persistence-unit name="studentPersistenceUnit">
        <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
        <class>no.kalli.model.Klasse</class>
        <class>no.kalli.model.Student</class>
        <properties>
            <property name="eclipselink.jdbc.url" value="jdbc:postgresql://kalliainen.ddns.net:5432/kalli"/>
            <property name="eclipselink.jdbc.driver" value="org.postgresql.Driver"/>
            <property name="eclipselink.jdbc.user" value="kalli"/>
            <property name="eclipselink.jdbc.password" value="kalli"/>
        </properties>
    </persistence-unit>
</persistence>
